".text.xml":
# xml.ui.core
	"xml.ui.core.Fragment":
		"prefix": "Fragment"
		"body":
			"""
			<core:Fragment
				fragmentName="${1:}" type="XML" />
			"""
	"xml.ui.core.FragmentDefinition":
		"prefix": "FragmentDefinition"
		"body":
			"""
			<core:FragmentDefinition xmlns="sap.m" xmlns:l="sap.ui.layout" xmlns:core="sap.ui.core">
				$0
			</core>
			"""
	"xml.ui.core.View":
		"prefix": "View"
		"body":
			"""
			<core:View controllerName="${1:}" xmlns:core="sap.ui.core" xmlns:form="sap.ui.layout.form" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:l="sap.ui.layout" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m">
				${2:}
			</core:View>
			"""
#xml.ui.layout
	"xml.ui.layout.BlockLayout":
		"prefix": "BlockLayout"
		"body":
			"""
			<l:BlockLayout background="${1:}">
				<l:BlockLayoutRow>
					<l:BlockLayoutCell
						width="${2:}"
						title="${3:}">
						${4:}
					</l:BlockLayoutCell>
				</l:BlockLayoutRow>
			</l:BlockLayout>
			"""
	"xml.ui.layout.BlockLayoutCell":
		"prefix": "BlockLayoutCell"
		"body":
			"""
			<l:BlockLayoutCell
				width="${1:}"
				title="${2:}">
				${3:}
			</l:BlockLayoutCell>
			"""
	"xml.ui.layout.BlockLayoutRow":
		"prefix": "BlockLayoutRow"
		"body":
			"""
			<l:BlockLayoutRow>
				${1:}
			</l:BlockLayoutRow>
			"""
	"xml.ui.layout.DynamicSideContent":
		"prefix": "DynamicSideContent"
		"body":
			"""
			<l:DynamicSideContent
				sideContentFallDown="${1:BelowM}"
				containerQuery="${2:true}"
				breakpointChanged="${3:updateToggleButtonState}">
					<l:mainContent>
						${4:}
					</l:mainContent>
					<l:sideContent>
						${5:}
					</l:sideContent>
			</l:DynamicSideContent>
			"""
	"xml.ui.layout.FixFlex":
		"prefix": "FixFlex"
		"body":
			"""
			<l:FixFlex>
				<l:fixContent>
					${1:}
				</l:fixContent>
				<l:flexContent>
					${2:}
				</l:flexContent>
			</l:FixFlex>
			"""
	"xml.ui.layout.Grid":
		"prefix": "Grid"
		"body":
			"""
			<l:Grid
				binding="${1:}"
				hSpacing="${2:2}"
				defaultSpan="${3:L6 M6 S10}">
				<l:content>
					${4:}
				</l:content>
			</l:Grid>
			"""
	"xml.ui.layout.GridData":
		"prefix": "GridData"
		"body":
			"""
			<l:GridData
				span="${1:L3 M3 S8}"
				linebreakL="${2:true}"
				linebreakM="${3:true}"
				linebreakS="${4:true}" />
			"""
	"xml.ui.layout.HorizontalLayout":
		"prefix": "HorizontalLayout"
		"body":
			"""
			<l:HorizontalLayout>
				${1:}
			</l:HorizontalLayout>
			"""
	"xml.ui.layout.Splitter":
		"prefix": "Splitter"
		"body":
			"""
			<l:Splitter height="${1:}">
				${2:}
			</l:Splitter>
			"""
	"xml.ui.layout.SplitterLayoutData":
		"prefix": "SplitterLayoutData"
		"body":
			"""
			<l:SplitterLayoutData size="${1:auto}" />
			"""
	"xml.ui.layout.VerticalLayout":
		"prefix": "VerticalLayout"
		"body":
			"""
			<l:VerticalLayout>
				${1:}
			</l:VerticalLayout>
			"""
#xml.ui.layout.form
	"xml.ui.layout.form.SimpleForm":
		"prefix": "SimpleForm"
		"body":
			"""
			<form:SimpleForm
				columnsL="${1:}"
				columnsM="${2:}"
				columnsS="${3:}"
				editable="${4:true}"
				layout="ResponsiveGridLayout">
				<form:content>
					<core:Title text=""/>
					<Label text=""/>
					<Input />${5:}
				</form:content>
			</form:SimpleForm>
			"""
# xml.mvc
	"xml.mvc.View":
		"prefix": "xmlView"
		"body":
			"""
			<mvc:View
				xmlns:mvc="sap.ui.core.mvc"
				displayBlock="true"
				xmlns="sap.m">
				$0
			</mvc:View>
			"""
# xml.m Alphabetical order, A:
	"xml.m.ActionListItem":
		"prefix":"ActionListItem"
		"body":
			"""
			<ActionListItem text="${1:}" />
			"""
	"xml.m.ActionSelect":
		"prefix":"ActionSelect"
		"body":
			"""
			<ActionSelect
				items="{
					path: '${1:}',
					sorter: { path: '${2:}' }
				}">
				<items>
					<core:Item key="{${3:}}" text="{${4:}}" />
				</items>
			</ActionSelect>
			"""
	"xml.m.ActionSheet":
		"prefix":"ActionSheet"
		"body":
			"""
			<ActionSheet
			title="${1:textProperty}"
			showCancelButton="${2:true}"
			cancelButtonText="${3:Cancel}"
			placement="Bottom">
				<buttons>
					<Button text="Reject" icon="sap-icon://decline" />${4:}
				</buttons>
			</ActionSheet>
			"""
	"xml.m.App":
		"prefix":"App"
		"body":
			"""
			<App id="${1:}" showHeader="${2:false}">
			</App>
			"""
# xml.m Alphabetical order, B:
	"xml.m.Bar":
		"prefix":"Bar"
		"body":
			"""
			<Bar>
				<contentLeft>
					${1:}
				</contentLeft>
				<contentMiddle>
					${2:}
				</contentMiddle>
				<contentRight>
					${3:}
				</contentRight>
			</Bar>
			"""
	"xml.m.Breadcrumbs":
		"prefix":"Breadcrumbs"
		"body":
			"""
			<Breadcrumbs currentLocationText="Laptop">
				<Link press="onPress" text="${1:}"/>
				<Link press="onPress" text="${2:}"/>
			</Breadcrumbs>
			"""
	"xml.m.BusyDialog":
		"prefix":"BusyDialog"
		"body":
			"""
			<BusyDialog
				title="${1:}"
				text="${2:}"
				showCancelButton="${3:true}"
				close="onDialogClosed"/>
