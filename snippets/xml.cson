				customIconWidth="${4:44px}"
				customIconHeight="${5:44px}"
				customIconRotationSpeed="${6:1000}"/>
			"""
	"xml.m.Button":
		"prefix":"Button"
		"body":
			"""
			<Button type="${1:}" press="${2:}" text="${3:}" icon="${4:}"/>
			"""
# xml.m Alphabetical order, C:
	"xml.m.Carousel":
		"prefix":"Carousel"
		"body":
			"""
			<Carousel loop="${1:true}" pageIndicatorPlacement="${2:Bottom}" showPageIndicator="${3:true}">
				<pages>
					<Image src="{img>/products/pic1}" alt="Example picture of speakers"/>
					<Image src="{img>/products/pic2}" alt="Example picture of USB flash drive"/>
					${2:}
				</pages>
			</Carousel>
			"""
	"xml.m.CheckBox":
		"prefix":"CheckBox"
		"body":
			"""
			<CheckBox text="${1:}" selected="${2:}" />
			"""
	"xml.m.ComboBox":
		"prefix":"ComboBox"
		"body":
			"""
			<ComboBox
				showSecondaryValues="${1:true}"
				selectedKey="{ path: '${2:}' }"
				items="{
					path: '${3:}',
					sorter: { path: '${4:}' }
				}">
				<core:ListItem key="{${5:}}" text="{${6:}}" additionalText="{${7:}}"/>
			</ComboBox>
			"""
	"xml.m.CustomList":
		"prefix":"CustomList"
		"body":
			"""
			<List items="{path: '${1:}'}" >
				<CustomListItem>
					${2:}
				</CustomListItem>
			</List>
			"""
# xml.m Alphabetical order, D:
	"xml.m.DatePicker":
		"prefix":"DatePicker"
		"body":
			"""
			<DatePicker displayFormat="${1:yyyy-MM-dd}" value="${2:}" valueFormat="${3:yyyy-MM-dd}" placeholder="${4:yyyy-MM-dd}"/>
			"""
	"xml.m.DateRangeSelection":
		"prefix":"DateRangeSelection"
		"body":
			"""
			<DateRangeSelection
				delimiter="{path:'${1:}'}"
