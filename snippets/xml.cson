						key="{${9:}}"
						multiselect="${10:true}"
						listClose="${11:handleListClose}"
						items="{${12:}}" >
						<items>
							<FacetFilterItem
								text="{${13:}}"
								key="{${14:}}"
								count="{${15:}}" />
						</items>
					</FacetFilterList>
