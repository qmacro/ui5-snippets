				secondDateValue="{path:'${3:}'}"
				displayFormat="{path:'${4:}'}"
				change="${5:handleChange}"
			/>
			"""
	"xml.m.Dialog":
		"prefix":"Dialog"
		"body":
			"""
			<Dialog title="${1:}" class="sapUiPopupWithPadding" contentHeight="${2:30rem}">
		        <content>
		            ${3:}
		        </content>
		        <buttons>
		            <Button text="${4:}" press="${5:handlePress}"/>
		        </buttons>
		    </Dialog>
